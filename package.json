{
    "name": "tools-angular",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng build && node server.js",
        "build": "ng build",
        "dev": "ng serve",
        "test": "jest",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "ghpages-production": "ng build --base-href=\"/\" && angular-cli-ghpages --dir dist/tools-angular --cname noplaceleft.tools --repo=https://GH_TOKEN@github.com/NoPlaceLeft/tools-client-production.git --name=\"GH_USERNAME\" --email=\"GH_EMAIL\"",
        "ghpages": "ng build --base-href=\"/\" && angular-cli-ghpages --dir dist/tools-angular --cname dev.noplaceleft.tools  --no-silent"
    },
    "private": true,
    "jest": {
        "preset": "jest-preset-angular",
        "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts",
        "setupFiles": [
            "jest-localstorage-mock"
        ],
        "moduleNameMapper": {
            "@shared/(.*)": "<rootDir>/src/app/shared/$1",
            "@core/(.*)": "<rootDir>/src/app/core/$1",
            "@admin/(.*)": "<rootDir>/src/app/admin/$1"
        }
    },
    "dependencies": {
        "@agm/core": "^1.0.0-beta.5",
        "@angular/animations": "^7.1.0",
        "@angular/cdk": "^7.1.0",
        "@angular/common": "^7.1.0",
        "@angular/compiler": "^7.1.0",
        "@angular/core": "^7.1.0",
        "@angular/flex-layout": "^7.0.0-beta.19",
        "@angular/forms": "^7.1.0",
        "@angular/http": "^7.1.0",
        "@angular/material": "^7.1.0",
        "@angular/material-moment-adapter": "^7.1.0",
        "@angular/platform-browser": "^7.1.0",
        "@angular/platform-browser-dynamic": "^7.1.0",
        "@angular/platform-server": "^7.1.0",
        "@angular/router": "^7.1.0",
        "@types/d3": "^5.0.1",
        "@types/file-saver": "^2.0.0",
        "@types/lodash": "^4.14.119",
        "core-js": "^2.5.7",
        "d3": "^5.7.0",
        "file-saver": "^2.0.0",
        "i18next": "^12.1.0",
        "i18next-browser-languagedetector": "^2.2.4",
        "jest": "^23.6.0",
        "jest-preset-angular": "^6.0.2",
        "koa": "^2.6.2",
        "koa-compress": "^3.0.0",
        "koa-conditional-get": "^2.0.0",
        "koa-etag": "^3.0.0",
        "koa-send": "^5.0.0",
        "koa-static": "^5.0.0",
        "lodash-es": "^4.17.11",
        "moment": "^2.22.2",
        "moment-timezone": "^0.5.23",
        "ng-bullet": "^1.0.3",
        "node-sass": "^4.10.0",
        "roboto-fontface": "^0.10.0",
        "rxjs": "^6.3.3",
        "rxjs-compat": "^6.3.3",
        "uuid": "^3.3.2",
        "xlsx": "^0.14.1",
        "zone.js": "^0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.12.0",
        "@angular/cli": "^7.1.2",
        "@angular/compiler-cli": "^7.1.0",
        "@angular/language-service": "^7.1.0",
        "@types/googlemaps": "^3.30.16",
        "@types/jest": "^23.3.10",
        "@types/node": "^10.12.10",
        "@types/uuid": "^3.4.4",
        "codelyzer": "^4.5.0",
        "jest-localstorage-mock": "^2.3.0",
        "protractor": "^5.4.1",
        "rxjs-tslint": "^0.1.6",
        "ts-node": "~7.0.1",
        "tslint": "^5.11.0",
        "typescript": "^3.1.6",
        "webpack": "^4.26.1"
    }
}
